{
    "title": "Editor",
    "rules": [
        {
            "description": "BackSpace",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Delete",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Enter",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ]
                }
            ]
        },
        {
            "description": "OneLineDelete",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "delete_or_backspace",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "OneWordDelete",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace",
                            "modifiers": [
                                "command",
                                "shift"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "DeleteAllForward",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow",
                            "modifiers": [
                                "command",
                                "shift"
                            ]
                        },
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                }
            ]
        },
        {
            "description": "SelectBackwardOneWord",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "option",
                                "shift"
                            ]
                        },
                        {
                            "key_code": "c",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "NewLine",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "return_or_enter",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "end"
                        },
                        {
                            "key_code": "return_or_enter"
                        }
                    ]
                }
            ]
        },
        {
            "description": "NewLineYank",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "v",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "command"
                            ]
                        },
                        {
                            "key_code": "return_or_enter"
                        },
                        {
                            "key_code": "v",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}