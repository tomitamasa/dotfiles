{
    "title": "AppSwitcher",
    "rules": [
        {
            "description": "PreviousApp",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "r",
                        "modifiers": {
                            "mandatory": ["right_option"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": ["option"]
                        }
                    ]
                }
            ]
        },
        {
            "description": "ReverseApp",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": ["right_option"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": ["option", "shift"]
                        }
                    ]
                }
            ]
        }
    ]
}